<?php if ($this->getLinks()): ?>
<ul class="breadcrumbs">
<?php foreach ($this->getLinks() as $_link): ?>
<li>
	<?php if ($_link->getUrl()): ?>
		<a href="<?php echo $_link->getUrl(); ?>"><?php echo $_link->getLabel();?></a>
	<?php else: ?>
		<?php echo $_link->getLabel(); ?>
	<?php endif; ?>
	&raquo;
</li>
<?php endforeach; ?>
</ul>
<?php endif;?>